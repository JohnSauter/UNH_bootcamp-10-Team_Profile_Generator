<!doctype html>
<html class="no-js" lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="./assets/css/foundation.css">
  <link rel="stylesheet" href="./assets/css/app.css">
  <title>Team Profile</title>
</head>

<body>
  <header id="header">
    <h1>My Team</h1>
  </header>

  <main>
    <div class="grid-container">
      <section id="team_members" class="grid-x">

        {{#employees}}
        <div class="card medium-4 large-3">
          <div class="card-divider team_member_name">
            {{name}}
          </div>
          <div class="card-divider team_member_role">
            {{getRole}}
          </div>
          <div class="card section team_member_info">
              <p>ID: {{id}}</p>
          </div>
          <div class="card section team_member_info">
            <p>e-mail: 
              <a href="mailto:{{email}}">
              {{email}}</a></p>
          </div>
          <div class="card section team_member_info">
            {{#github}}<p>GitHub: <a href="https://github.com/{{github}}" 
                target="_blank" rel="noopener noreferrer">
                {{github}}</a></p>{{/github}}
                {{#officeNumber}}<p>Office: {{officeNumber}}</p>{{/officeNumber}}
                {{#school}}<p>School: {{school}}</p>{{/school}}
          </div>
        </div>
        {{/employees}}
        
      </section> <!-- team members-->
    </div> <!-- grid container -->
  </main>

  <script src="./assets/js/vendor/jquery.js"></script>
  <script src="./assets/js/vendor/what-input.js"></script>
  <script src="./assets/js/vendor/foundation.js"></script>
  <script src="./assets/js/app.js"></script>

</body>

</html>